{
  "name": "mtproto-storage-browser",
  "version": "0.1.5",
  "description": "Save telegram-mtproto session in localStorage",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ -d lib -s",
    "build:es": "babel src/ -d es -s",
    "clean": "rimraf lib/ && rimraf es/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "flow-copy-source src lib && flow-copy-source src es",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "lib",
    "src",
    "es",
    "index.d.ts"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "localforage": "^1.5.0",
    "mtproto-logger": "^0.1.9",
    "mtproto-shared": "^0.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cross-env": "^5.0.5",

    "flow-copy-source": "^1.2.1",
    "rimraf": "^2.6.2"
  },
  "author": "Zero Bias",
  "maintainers": [
    {
      "name": "Zero Bias",
      "email": "ribkatt@gmail.com"
    },
    {
      "name": "goodmind",
      "email": "andwebar@gmail.com"
    }
  ],
  "license": "MIT"
}
